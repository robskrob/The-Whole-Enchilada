{
  "manifest_version": 2,
  "name": "Recipe Saver",
  "version": "0.1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://d81e7962.ngrok.io; object-src 'self'",
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.0.min.js", "content.js"]
    }
  ]
}
